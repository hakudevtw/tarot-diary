{
  "name": "tarot-diary",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.2.0",
  "engines": {
    "node": "20.10.0",
    "pnpm": "9.2.0"
  },
  "scripts": {
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next .swc out coverage",
    "lint": "next lint",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "test": "vitest run",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://127.0.0.1:6006 test-storybook",
    "commit": "cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@logtail/pino": "^0.4.22",
    "@t3-oss/env-nextjs": "^0.10.1",
    "next": "14.2.3",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/types": "^19.0.3",
    "@next/bundle-analyzer": "^14.2.3",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/nextjs": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@storybook/test-runner": "^0.18.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/expect": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vitest": "~0.4.1",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "jiti": "^1.21.3",
    "lint-staged": "^15.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.3.1",
    "rimraf": "^5.0.7",
    "start-server-and-test": "^2.0.4",
    "storybook": "^8.1.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vite": "^5.2.13",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "vitest-fail-on-console": "^0.7.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
